/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

page{   
    background:#f1f5f8;
    width:100%;
    height:100%;
}

/* 占位图 */
.noDataBox {
  width: 500rpx;
  height: 500rpx;
  margin: 177rpx auto 0 auto;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}

.noData {
  width: 500rpx;
  height: 374rpx;
}

.noHas {
  font-size: 26rpx;
  color: #999;
}

/* 回到首页按钮 */
.movbox{width:100%;height:100%;background:transparent;position:absolute;top:0;left:0;z-index:1;}
.backBtn{width:100rpx;height:100rpx;border-radius:50rpx;background:#313131;text-align:center;display:flex;flex-direction:column;justify-content:center;align-items:center;}
.homeimg{width:40rpx;height:40rpx;margin:0 auto;}
.shouye{color:#fff;font-size:20rpx;}